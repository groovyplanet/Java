package quiz01;

import java.util.Arrays;
class Solution {
    public int solution(String my_string, String is_suffix) {
        int answer = 0;
        for(int i =0 ; i<my_string.length(); i++) {
        	String x=my_string.substring(i,my_string.length());

        	if(x.equals(is_suffix)) {
        		answer=0;
        		break;
        	}else {
        		answer=-1;
        		break;
        	}
        }
        
        
        return answer;
    }
}