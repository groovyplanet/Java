package quiz01;

import java.util.Arrays;
class Solution {
    public int solution(String my_string, String is_prefix) {
        int answer = 0;
        int index=is_prefix.length();
        String a[]=my_string.split("");
        String b[]=is_prefix.split("");
        for(int i=0; i<index; i++) {
        	if(a[i].equals(b[i])) {
        		answer=1;
        	}else {
        		break;
        	}
        }

		return answer;
    }
}