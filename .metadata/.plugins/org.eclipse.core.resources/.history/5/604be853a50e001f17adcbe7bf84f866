package quiz01;

public class Quiz18 {

	public static void main(String[] args) {
		
		//for문 사용
		
		// 1. 7~100까지 정수 중에서 7의 배수를 가로로 출력
		
		// 2. 1~100까지 정수 중에서 9의 배수의 개수를 출력
		
		//3. 50~100까지 두 수 사이의 합계를 출력
		
		//4. A~Z 문자열의 합 출력 ABCDEF..Z = 65
		
		int sum=0;

		
		for(int i = 7; i<=100; i++) {
			if(i%7==0) {
				System.out.print(i+" ");
			}
		}
		
		for(int a = 1 ; a<=100; a++) {
			if(a%9==0) {
				sum++;
			}
			
		}
		System.out.println("\n"+sum);
		
		for(int b = 50; b<=100; b++) {
			sum+=b;
			b++;
		}
		System.out.println("\n"+sum);
		
	
		
		for(char i='A'; i<='Z'; i++) {
			sum+=i;
		}System.out.println("\n"+sum);
		
	   for(char i='A'; i<='Z'; i++) {
		System.out.print("\n"+i);
	}

	}
}
