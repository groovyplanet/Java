package day17.thread.basic;

public class MyThread implements Runnable{

	private int num;
	
	@Override
	public void run() {
		
		for(int i =0; i<=10; i++) {
			num++; //10번 증가.
			
			System.out.println(num);
			
			try {
				Thread.sleep(1000); //스레드 1초 멈춤
					
			} catch (Exception e) {
				// TODO: handle exception
			}
			
		
			
			
		}
		
		
	}

}
